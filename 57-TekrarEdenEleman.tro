void KopyaBul;
var
  I, J : Integer;
{
  Kopya = -1; 

  for I = 0 to High(dizi) - 1
  {
    for J = I + 1 to High(dizi)
    {
      if dizi[I] == dizi[J] 
      {
        Kopya = dizi[I]; 
        Exit;
      }
    }
  }
}

